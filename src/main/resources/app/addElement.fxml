      <TextField fx:id="categoryElem" alignment="CENTER" layoutX="29.0" layoutY="75.0" promptText="CATEGORY" />
      <Button layoutX="118.0" layoutY="194.0" mnemonicParsing="false" onAction="#addElem" text="ADD" />
      <Button layoutX="45.0" layoutY="194.0" mnemonicParsing="false" onAction="#close" text="CLOSE" />
      <TextField fx:id="nameElem" alignment="CENTER" layoutX="30.0" layoutY="109.0" promptText="NUME" />
      <TextField fx:id="priceElem" alignment="CENTER" layoutX="29.0" layoutY="144.0" promptText="PRICE" />
